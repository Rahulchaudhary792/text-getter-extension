{
	"name": "Title Picker",
	"version": "1.0",
	"description": "Gives title of the current tab",
	"manifest_version": 3,
	"action": {
		"default_popup": "popup.html"
	},
	"icons": {
		"16": "/images/icon16.png",
		"48": "/images/icon48.png",
		"128": "/images/icon128.png"
	},
	"permissions": [
		"tabs",
		"storage"
	]
}